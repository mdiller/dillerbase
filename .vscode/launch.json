{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Dev",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceRoot}/server",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run-script", "dev"
			]
		},
		{
			"name": "Loader",
			"type": "python",
			"request": "launch",
			"program": "loader/loader.py",
			"console": "integratedTerminal"
		},
	]
}